  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.6",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "bcrypt": "^4.0.1",
    "chai": "^4.2.0",
    "chromedriver": "^80.0.1",
    "connect-session-knex": "^1.6.0",
    "eslint": "^6.8.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.17.0",
    "geckodriver": "^1.19.1",
    "graphql": "^14.6.0",
    "knex": "^0.20.10",
    "lint": "^0.7.0",
    "mocha": "^7.0.0",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.4",
    "pg": "^7.18.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-dropdown": "^1.7.0",
    "react-select": "^3.0.8",
